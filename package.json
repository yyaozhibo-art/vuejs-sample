{
  "name": "my-vue-app",
  "version": "0.0.1",
  "homepage": "https://github.com/kristiyan-velkov/frontend-prod-dockerfiles",
  "author": {
    "name": "Kristiyan Velkov",
    "url": "https://www.linkedin.com/in/kristiyan-velkov-763130b3/"
  },
  "bugs": {
    "url": "https://github.com/kristiyan-velkov/frontend-prod-dockerfiles/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/kristiyan-velkov"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest run --environment jsdom --reporter verbose",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "3.5.21",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.3.1",
    "@vitejs/plugin-vue": "6.0.4",
    "@vitejs/plugin-vue-jsx": "5.1.1",
    "@vitest/eslint-plugin": "1.3.9",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^15.1.0",
    "eslint": "^9.35.0",
    "eslint-plugin-cypress": "^5.1.1",
    "eslint-plugin-vue": "^10.4.0",
    "jiti": "^2.5.1",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.1.0",
    "typescript": "~5.9.2",
    "vite": "7.1.5",
    "vite-plugin-vue-devtools": "^8.0.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  }
}
